{
   "parser": "@typescript-eslint/parser",
   "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
   },
   "plugins": [
      "@typescript-eslint"
   ],
   "rules": {
      "no-unused-vars": "warn",
      "prefer-const": "error",
      "no-unused-expressions": "warn",
      "no-undef": "error",
      "no-console": "warn"
   },
   "env": {
      "browser": true,
      "es6": true,
      "node": true
   },
   "globals": {
      "process": "readonly"
   }
}
